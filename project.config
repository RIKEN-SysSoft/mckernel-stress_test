[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Masamichi Takagi (masamichitakagi)
[access "refs/for/refs/*"]
	read = group user/Masamichi Takagi (masamichitakagi)
	push = group user/Masamichi Takagi (masamichitakagi)
	label-Code-Review = -2..+2 group user/Masamichi Takagi (masamichitakagi)
	label-Verified = -1..+1 group user/Masamichi Takagi (masamichitakagi)
	rebase = group user/Masamichi Takagi (masamichitakagi)
	removeReviewer = group user/Masamichi Takagi (masamichitakagi)
	submit = group user/Masamichi Takagi (masamichitakagi)
[access "refs/heads/*"]
	read = group user/Masamichi Takagi (masamichitakagi)
	create = group user/Masamichi Takagi (masamichitakagi)
	push = +force group user/Masamichi Takagi (masamichitakagi)
	pushMerge = group user/Masamichi Takagi (masamichitakagi)
[access "refs/tags/*"]
	push = group user/Masamichi Takagi (masamichitakagi)
	push = +force group user/Masamichi Takagi (masamichitakagi)
[receive]
	requireChangeId = true
[submit]
	action = rebase if necessary
	mergeContent = true
